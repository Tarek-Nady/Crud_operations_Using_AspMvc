<div class="form-group">
    <label class="col-form-label" asp-for="UserName"></label>
    <input type="text" class="form-control" asp-for="UserName" readonly />

</div>

<div class="form-group">
    <label class="col-form-label" asp-for="Email"></label>
    <input type="text" class="form-control" asp-for="Email" readonly />

</div>

<div class="form-group">
    <label class="col-form-label" asp-for="PhoneNumber"></label>
    <input class="form-control" asp-for="PhoneNumber" readonly />

</div>